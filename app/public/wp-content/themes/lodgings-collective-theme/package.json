{
  "name": "lodgings-collective-theme",
  "version": "1.0.0",
  "description": "Lodgings Collective website WordPress theme",
  "main": "index.js",
  "prettier": "@wordpress/prettier-config",
  "scripts": {
    "dev": "mix",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production",
    "lint": "eslint \"./**/*.js\" --fix",
    "serv": "git rm -r --cached wp-content/themes/lodgings-collective-theme/assets/src wp-content/themes/lodgings-collective-theme/.babelrc wp-content/themes/lodgings-collective-theme/.editorconfig wp-content/themes/lodgings-collective-theme/.eslintignore wp-content/themes/lodgings-collective-theme/.eslintrc wp-content/themes/lodgings-collective-theme/.gitignore wp-content/themes/lodgings-collective-theme/composer.json wp-content/themes/lodgings-collective-theme/package.json wp-content/themes/lodgings-collective-theme/webpack.mix.js",
    "transl": "wp i18n make-pot . languages/lodgings_collective_theme.pot --exclude=vendor,node_modules --headers='Name: theme_name\\n' --domain=lodgings_collective_theme --skip-js"
  },
  "repository": {},
  "keywords": [
    "wordpress",
    "theme",
    "translation-ready"
  ],
  "author": "Lodgings Collective",
  "license": "GPL-3.0",
  "devDependencies": {
    "@wordpress/babel-preset-default": "^6.0.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.1.3",
    "@wordpress/eslint-plugin": "^9.0.5",
    "@wordpress/prettier-config": "^1.0.4",
    "browser-sync": "^2.27.11",
    "browser-sync-webpack-plugin": "^2.3.0",
    "laravel-mix": "^6.0.49",
    "normalize-scss": "^7.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.57.1",
    "sass-loader": "^12.6.0"
  },
  "dependencies": {
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21"
  }
}
